  "main": "server.js",
  "scripts": {
    "clean": "rimraf assets",
    "prebuild": "npm run clean",
    "build": "webpack --config ./webpack.production.config.js --progress --profile",
    "lint": "npm run lint:api && npm run lint:app && npm run lint:lib",
    "lint:app": "eslint --ext .js --ext .jsx --ignore-path .gitignore app",
    "lint:api": "eslint --ext .js --ignore-path .gitignore api",
    "lint:lib": "eslint --ext .js --ignore-path .gitignore lib",
    "watch": "nodemon --exec npm start",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 server.js",
    "postinstall": "if [ -n \"$BUILD_ASSETS\" ]; then npm run build; fi",
    "pretest": "npm run lint",
    "test": "mocha"
  },
