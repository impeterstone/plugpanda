{
  "name": "panda",
  "author": "Peter Shih <ptshih@gmail.com>",
  "version": "1.0.7",
  "description": "Panda",
  "private": true,
  "dependencies": {
    "accounting": "0.4.1",
    "babel": "6.3.26",
    "babel-core": "6.3.26",
    "babel-eslint": "4.1.6",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babelify": "7.2.0",
    "bcrypt": "0.8.5",
    "bluebird": "3.1.1",
    "body-parser": "1.14.2",
    "browserify": "12.0.1",
    "chance": "0.8.0",
    "compression": "1.6.0",
    "cors": "2.7.1",
    "envify": "3.4.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.13.1",
    "express": "4.13.3",
    "fastclick": "1.0.6",
    "fbjs": "0.6.0",
    "fieldkit": "0.1.9",
    "flux": "2.1.1",
    "hbs": "4.0.0",
    "history": "1.17.0",
    "iron_worker": "0.1.6",
    "lodash": "3.10.1",
    "moment": "2.11.0",
    "morgan": "1.6.1",
    "muni": "1.2.7",
    "nconf": "0.8.2",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1",
    "nprogress": "0.2.0",
    "react": "0.14.5",
    "react-dom": "0.14.5",
    "react-highcharts": "6.0.0",
    "react-router": "1.0.3",
    "react-tap-event-plugin": "0.2.1",
    "response-time": "2.3.1",
    "serve-favicon": "2.3.0",
    "store": "1.3.20",
    "superagent": "1.6.1",
    "twilio": "2.8.0",
    "uglify-js": "2.6.1"
  },
  "devDependencies": {
    "live-reload": "*",
    "parallelshell": "*",
    "watchify": "*"
  },
  "main": "server.js",
  "browser": "app.js",
  "scripts": {
    "prebuild:js": "npm run lint",
    "pretest": "npm run lint",
    "preinstall": "npm run clean",
    "postinstall": "npm run build",
    "test": "mocha",
    "start": "node --harmony --optimize_for_size --max_old_space_size=460 --gc_interval=100 server.js",
    "start:watch": "nodemon --harmony -e js -w server.js -w api/ -w lib/ server.js",
    "clean": "rm -rf assets/*",
    "bump": "git checkout master && git pull origin master && npm version patch && git push --follow-tags",
    "lint:app": "eslint --ext .js --ext .jsx --ignore-path .gitignore app",
    "lint:api": "eslint --ext .js --ignore-path .gitignore api",
    "lint:lib": "eslint --ext .js --ignore-path .gitignore lib",
    "lint": "npm run lint:api && npm run lint:app && npm run lint:lib",
    "live-reload": "live-reload assets/ --port 9091",
    "deploy": "git push heroku",
    "build": "npm run build:js && npm run build:css",
    "build:js": "mkdir -p assets/js && browserify --extension=.jsx app.js | uglifyjs -m -o assets/js/main.min.js",
    "build:css": "mkdir -p assets/css && node-sass --output-style compressed scss/main.scss assets/css/main.min.css",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:css' 'npm run live-reload'",
    "watch:js": "mkdir -p assets/js && watchify --extension=.jsx app.js -o assets/js/main.js -dv",
    "watch:css": "mkdir -p assets/css && node-sass -w scss/main.scss assets/css/main.css"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "engines": {
    "node": "5.3.0",
    "npm": "3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ptshih/panda.git"
  },
  "bugs": {
    "url": "https://github.com/ptshih/panda/issues"
  },
  "homepage": "https://github.com/ptshih/panda",
  "license": "MIT"
}
